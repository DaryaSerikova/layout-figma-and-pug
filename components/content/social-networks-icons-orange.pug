- var baseLink = '../assets/svg/'
- var networksIcons = ['vk2', 'tg2', 'inst2', 'wsup2'];

 
div.header__social-networks-icons.header__item
    a.header__social-networks.vk-icon(href='/')
      img.header__social-networks-img(
        src=baseLink + 'vk2.svg'
        alt='vk_icon2'
      )
    a.header__social-networks.tg-icon(href='/')
      img.header__social-networks-img(
        src=baseLink + 'tg2.svg'
        alt='tg_icon2'
      )
    a.header__social-networks.inst-icon(href='/')
      img.header__social-networks-img(
        src=baseLink + 'inst2.svg'
        alt='inst_icon2'
      )
    a.header__social-networks.wsup-icon(href='/')
      img.header__social-networks-img(
        src=baseLink + 'wsup2.svg'
        alt='wsup_icon2'
      )
